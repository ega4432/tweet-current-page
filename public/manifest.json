{
  "manifest_version": 3,
  "name": "Tweet Current Page",
  "description": "The simple chrome extension that allows you to tweet the site you are currently viewing.",
  "version": "0.1.0",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "action": {},
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [],
  "permissions": ["tabs", "activeTab"]
}
